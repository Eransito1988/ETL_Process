<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <DataSet Name="DataSet1">
    <Query>
      <DataSourceReference>DW_TheVoice</DataSourceReference>
      <DataSetParameters>
        <DataSetParameter Name="@StartDate">
          <ReadOnly>false</ReadOnly>
          <Nullable>false</Nullable>
          <OmitFromQuery>false</OmitFromQuery>
          <rd:DbType>Object</rd:DbType>
        </DataSetParameter>
        <DataSetParameter Name="@EndDate">
          <ReadOnly>false</ReadOnly>
          <Nullable>false</Nullable>
          <OmitFromQuery>false</OmitFromQuery>
          <rd:DbType>Object</rd:DbType>
        </DataSetParameter>
        <DataSetParameter Name="@CallType">
          <ReadOnly>false</ReadOnly>
          <Nullable>false</Nullable>
          <OmitFromQuery>false</OmitFromQuery>
          <rd:DbType>Object</rd:DbType>
        </DataSetParameter>
        <DataSetParameter Name="@CallTypeDescripion">
          <ReadOnly>false</ReadOnly>
          <Nullable>false</Nullable>
          <OmitFromQuery>false</OmitFromQuery>
          <rd:DbType>Object</rd:DbType>
        </DataSetParameter>
      </DataSetParameters>
      <CommandText>with cte as
(
select [Call Type Descripion], [Call ID], [Key Customer], Duration, [Billable Amount], ct.[Call Type], ct.[Category Of Call] ,Row_Number () over (partition by CT.[Call Type] order by CT.[Call Type] asc) 'Row Of Calls Per Types'
from Fact_Usage_Main UM join Dim_Call_Types CT
on UM.[Key Call Type] = CT.[Key Call Type]
join Calander_Table Cc
on um.[Key Call Date] = cc.DateKey
Where
	([Date] &gt;= @StartDate and [Date] &lt;= @EndDate) and
	[Call Type] in(@CallType) and
	[Call Type Descripion] in (@CallTypeDescripion)
),
cte2 as
(
select [Call Type Descripion], [Call ID], [Key Customer], [Call Type], Duration, [Billable Amount], max([Row Of Calls Per Types]) over (partition by [Call Type]) 'Sum Of Calls'
from cte
)
select [Call Type],[Call Type Descripion], sum(Duration) 'Duration Per Type', sum([Billable Amount]) [Billable Amount Per Type], count([Sum Of Calls])[Sum Of Calls Per Type] 
from cte2
group by [Call Type], [Call Type Descripion]
</CommandText>
    </Query>
    <Fields>
      <Field Name="Call_Type">
        <DataField>Call Type</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="Call_Type_Descripion">
        <DataField>Call Type Descripion</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="Duration_Per_Type">
        <DataField>Duration Per Type</DataField>
        <rd:TypeName>System.Int32</rd:TypeName>
      </Field>
      <Field Name="Billable_Amount_Per_Type">
        <DataField>Billable Amount Per Type</DataField>
        <rd:TypeName>System.Double</rd:TypeName>
      </Field>
      <Field Name="Sum_Of_Calls_Per_Type">
        <DataField>Sum Of Calls Per Type</DataField>
        <rd:TypeName>System.Int32</rd:TypeName>
      </Field>
    </Fields>
  </DataSet>
</SharedDataSet>